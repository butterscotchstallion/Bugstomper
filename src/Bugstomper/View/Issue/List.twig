{% include 'Header.twig' %}<table class="table table-bordered">    <thead>        <tr>            <td>Title</td>            <td>Status</td>            <td>Assigned</td>        </tr>    </thead>    <tbody>        {% for i in issues %}            <tr>                <td><a href="/i/{{ i.id }}">{{ i.title|e }}</a></td>                <td><a href="/i?sort=status">{{ i.statusName|e }}</a></td>                <td><a href="/u/{{ i.assignedToUserID }}">{{ i.assignedToUserLogin|e }}</a></td>            </tr>        {% endfor %}    </tbody></table>{% include 'Footer.twig' %}